@model StoreCheck.Models.Spr_ChannelRetail

@{
    ViewBag.Title = "Редактирование: Каналы реализации";
}


<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Каналы реализации</legend>

        @Html.HiddenFor(model => model.ID)

        <div class="editor-label">
            @Html.LabelFor(model => model.SBU)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.SBU)
            @Html.ValidationMessageFor(model => model.SBU)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Каналреализации)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Каналреализации)
            @Html.ValidationMessageFor(model => model.Каналреализации)
        </div>

        <p>
            <input type="submit" value="Сохранить" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Назад", "Spr_ChannelRetailList")
</div>
